					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#example">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a href="/home" class="navbar-brand"><img width="150" height= "45" src="/contents/image/black.jpg" ></a>
				</div>
				<div class="navbar-form navbar-right" id="example">
					Welcome <%=email%>
				  <ul class="nav navbar-nav">

					<!--	<li><a href="">Auction</a></li>-->
